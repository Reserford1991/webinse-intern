<layout>
    <adminhtml_webinse_company_index_index>
        <reference name="content">
            <block type="webinse_company/adminhtml_company" name="company"></block>
        </reference>
    </adminhtml_webinse_company_index_index>

    <adminhtml_webinse_company_index_new>        
        <update handle="editor"/>
        <reference name="head">
            <action method="addJs">
                <script>lib/jquery/jquery-1.10.2.min.js</script>
            </action>
            <action method="addJs">
                <script>lib/jquery/noconflict.js</script>
            </action>              
        </reference>              
        <reference name="content">            
            <block type="webinse_company/adminhtml_company_edit" name="webinse_company"/>
            <block type="webinse_company/adminhtml_countries" name="company" template="webinse_company/countries.phtml"/>        
        </reference>                
        <reference name="left">            
            <block type="webinse_company/adminhtml_company_edit_tabs" name="edit_tabs">                
                <block type="webinse_company/adminhtml_company_edit_tab_main" name="edit_tab_main"/>                                
                <block type="webinse_company/adminhtml_company_edit_tab_addres" name="edit_tab_addres"/>                                
                <action method="addTab">
                    <name>main_section</name>
                    <block>webinse_company/adminhtml_company_edit_tab_main</block>
                </action>                                
                <action method="addTab">
                    <name>addres_section</name>
                    <block>webinse_company/adminhtml_company_edit_tab_addres</block>
                </action>                            
            </block>                    
        </reference>            
    </adminhtml_webinse_company_index_new>
</layout>